            <HBox fx:id="gateway" alignment="CENTER" maxHeight="110.0" minHeight="140.0" prefHeight="140.0" BorderPane.alignment="CENTER">

      </HBox>
         </center>
         <bottom>
            <HBox fx:id="bottomRow" alignment="CENTER" maxHeight="100.0" minHeight="130.0" prefHeight="130.0" BorderPane.alignment="CENTER">

      </HBox>
