            <Font size="18.0" />
         </font>
         <padding>
            <Insets left="15.0" top="15.0" />
         </padding>
      </Label>
   </top>
   <bottom>
      <Pane maxHeight="50.0" maxWidth="500.0" minHeight="50.0" minWidth="500.0" prefHeight="50.0" prefWidth="500.0" BorderPane.alignment="CENTER" />
